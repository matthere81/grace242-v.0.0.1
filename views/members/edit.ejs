<% include ../partials/header %>

<h1>Edit <%= member.name %></h1>
<form action="/members/<%= member._id %>?_method=PUT" method="POST">
    <div class="form-group">
    <input class="form-control" type="text" name="member[name]" value="<%= member.name %>">
    </div>
    <div class="form-group">
    <input class="form-control" type="text" name="member[image]" value="<%= member.image %>">
     </div>
     <div class="form-group">
    <input class="form-control" type="text" name="member[description]" value="<%= member.description %>">
     </div>
     <div class="form-group">
    <button class="btn btn-large btn-primary btn-block">Submit</button>
    </div>
</form>